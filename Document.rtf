import java.awt.*;\par
public class analogclock extends Applet \{\par
    public void init() \{\par
       this.setSize(new Dimension(800,400));\par
       setBackground(new Color(50,50,50));\par
       new Thread()\{\par
           public void run()\{\par
               while(true)\{\par
                   repaint();\par
                   delayAnimation();\par
               \}\par
           \}\par
           \}.start();\par
       \}\par
        private void delayAnimation()\{\par
            try\{\par
                Thread.sleep(1000);\par
            \}\par
            catch(InterruptedException e)\{\par
                e.printStackTrace();\par
            \}\par
        \}\par
        public void paint(Graphics g)\{\par
            Calendar Time=Calendar.getInstance();\par
            int hour=Time.get(Calendar.HOUR_OF_DAY);\par
            int minute=Time.get(Calendar.MINUTE);\par
            int second=Time.get(Calendar.SECOND);\par
            if(hour>12)\{\par
                hour-=12;\par
            \}\par
            g.setColor(Color.white);\par
            g.fillOval(300,100,200,200);\par
            g.setColor(Color.black);\par
            g.drawString("12",390,120);\par
            g.drawString("9",310,200);\par
            g.drawString("6",400,290);\par
            g.drawString("3",480,200);\par
            double angle;\par
            int x,y;\par
            angle=Math.toRadians((15-second)*6);\par
            x=(int)(Math.cos(angle)*100);\par
            y=(int)(Math.sin(angle)*100);\par
            g.setColor(Color.red);\par
            g.drawLine(400,200,400+x,200-y);\par
            angle=Math.toRadians((15-minute)*6);\par
            x=(int)(Math.cos(angle)*80);\par
            y=(int)(Math.sin(angle)*80);\par
            g.setColor(Color.blue);\par
            g.drawLine(400,200,400+x,200-y);\par
            angle=Math.toRadians((15-(hour*5))*6);\par
            x=(int)(Math.cos(angle)*50);\par
            y=(int)(Math.sin(angle)*50);\par
            g.setColor(Color.black);\par
            g.drawLine(400,200,400+x,200-y);\par
        \}\par
\}\par
